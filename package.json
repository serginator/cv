{
  "name": "cv",
  "version": "1.0.0",
  "description": "My CV following FRESH standard",
  "main": "index.js",
  "scripts": {
    "start": "hackmyresume BUILD resume.json TO out/resume.all -t positive && mkdir -p dist && cp out/resume.html dist/index.html",
    "to-jrs": "hackmyresume CONVERT resume.json out/resume-jrs.json",
    "test": "hackmyresume VALIDATE resume.json",
    "analyze": "hackmyresume ANALYZE resume.json",
    "deploy": "gh-pages -d dist -m 'Pushed gh-pages'"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/serginator/cv.git"
  },
  "keywords": [
    "cv",
    "fresh",
    "fresca",
    "resume",
    "curr√≠culum",
    "vitae"
  ],
  "author": "Sergio Ruiz (@serginator)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/serginator/cv/issues"
  },
  "homepage": "https://serginator.github.io/cv",
  "devDependencies": {
    "gh-pages": "^0.10.0",
    "hackmyresume": "^1.8.0",
    "phantomjs": "^2.1.3"
  }
}
